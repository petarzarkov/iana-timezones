{
  "name": "timezone-db",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "description": "Auto generated timezones from iana db https://www.iana.org/time-zones",
  "author": {
    "name": "Petar Zarkov",
    "email": "pzarko1@gmail.com",
    "url": "https://github.com/petarzarkov"
  },
  "license": "MIT",
  "version": "0.1.0",
  "engines": {
    "node": ">= 22.15.0",
    "pnpm": ">= 10.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petarzarkov/iana-timezones.git"
  },
  "homepage": "https://github.com/petarzarkov/iana-timezones",
  "scripts": {
    "prepare": "node -e \"process.env.CI || process.env.DOCKER || process.exit(1)\" || husky",
    "generate": "tsx --env-file=.env src/generateTimezones.ts",
    "typecheck": "tsc --noEmit",
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "postbuild": "sh scripts/fix-types.sh && rm -rf dist/types/timezones.d.ts",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest dev",
    "lint": "eslint --cache src/**",
    "pretty": "prettier --cache --ignore-unknown --write .",
    "versioning": "tsx --env-file=.env src/version.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tsconfig/node22": "^22.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.4",
    "@types/tar-stream": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-v8": "3.1.3",
    "csv-parse": "^5.6.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "rollup": "^4.40.2",
    "tar-stream": "^3.1.7",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
  "keywords": [
    "iana",
    "timezones",
    "timezone",
    "utc",
    "offset",
    "gmt",
    "list",
    "db"
  ]
}
